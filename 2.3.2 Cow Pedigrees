#include <bits/stdc++.h>
using namespace std;
#define cin fin
#define cout fout
int main(){
    ifstream fin("nocows.in");
    ofstream fout("nocows.out");
    int n, m;
    cin>>n>>m;
    vector<vector<int>> dp(n+1, vector<int>(m+1, 0));//dp[i][j] is the number of pedigree structures with depth j with i cows
    vector<vector<int>> dpsum(n+1, vector<int>(m+1, 0));//dpsum[i][j] is the number of pedigree structures with i cows with at most j depth
    dp[1][1]=1;
    for(int i = 1; i<=n; i+=2){
        for(int j = 1; j<=m; j++){
            dpsum[i][j]=dpsum[i][j-1];
            for(int k = 1; k<=i-2; k+=2){
                dp[i][j]+=(dp[k][j-1]*dpsum[i-1-k][j-1]);//One subtree has to be of depth j-1 while the other subtree just has to be of at most depth j-1
                dp[i][j]+=(dpsum[k][j-1]*dp[i-1-k][j-1]);
                dp[i][j]-=(dp[k][j-1]*dp[i-1-k][j-1]);
                dp[i][j]%=9901;
            }
            dpsum[i][j]+=dp[i][j];//Summing
            dpsum[i][j]%=9901;
        }
    }
    cout<<dp[n][m]<<'\n';

    return 0;
}
